{
  "sourceMaps": true,
  "jsc": {
    "loose": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2020",
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "misc/*": ["cypress/misc/*"]
    }
  },
  "module": {
    "type": "commonjs"
  }
}
